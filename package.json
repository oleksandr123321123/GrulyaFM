    "express": "^4.21.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@bubblewrap/cli": "^1.20.0",
    "eslint": "^8.57.0",
    "web-push": "^3.6.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }

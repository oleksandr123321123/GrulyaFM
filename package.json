{
  "name": "grulyafm",
  "version": "1.0.0",
  "description": "GrulyaFM - Global Radio with 200+ stations from 22 countries",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.mjs",
    "build": "echo 'Static site, no build needed'",
    "lint": "eslint . --ext .js --max-warnings 0",
    "lint:fix": "eslint . --ext .js --fix",
    "test": "echo 'No tests yet'",
    "lighthouse": "lhci autorun",
    "audit": "npm audit --audit-level=moderate",
    "check-secrets": "echo 'Run: git log --all --full-history -- \"*.keystore\"'",
    "check-ready": "node check-deployment-ready.cjs",
    "build:android": "bubblewrap build",
    "generate-vapid": "node generate-vapid-keys.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oleksandr123321123/GrulyaFM.git"
  },
  "keywords": [
    "radio",
    "streaming",
    "pwa",
    "music",
    "global",
    "web-audio"
  ],
  "author": "GrulyaFM Team",
  "license": "MIT",
